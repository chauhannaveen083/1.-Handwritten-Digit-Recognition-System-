ain=x_train.reshape(60000,28,28,1)\n","x_test=x_test.reshape(10000,28,28,1)\n","\n","\n","y_train=to_categorical(y_train,10)\n","y_test=to_categorical(y_test,10)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"4YO9mfJTzhGK","colab_type":"code","outputId":"6a56bbad-4ce1-4321-b552-5b11cf81e00e","executionInfo":{"status":"ok","timestamp":1566678728443,"user_tz":-330,"elapsed":10643,"user":{"displayName":"BIKASH DAS","photoUrl":"https://lh3.googleusercontent.com/a-/AAuE7mA5elKleZiF4F9ZYrpPJodn0IB6EODss3JRtB3ZxA=s64","userId":"13209619777831388550"}},"colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["\n","#scaling of images\n","x_train=x_train/255\n","x_test=x_test/255\n","\n","#n_pixels=x_train.shape[1]*x_train.shape[2] #=28 * 28\n","\n","#x_train=x_train.reshape(x_train.shape[0],n_pixels)\n","#x_test=x_test.reshape(x_test.shape[0],n_pixels)\n","\n","\n","print(x_train.shape)\n"],"execution_count":0,"outputs":[{"output_type":"stream","text":["(60000, 28, 28, 1)\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"hXkzFt9r14ka","colab_type":"code","colab":{}},"source":["def lenet_model():\n","  model=Sequential()\n","  \n","  model.add(Conv2D(30,(5,5),input_shape=(28,28,1),activation='relu'))\n","  \n","  model.add(MaxPooling2D(pool_size=(2,2)))\n","  model.add(Dropout(0.5))\n","  \n","  model.add(Conv2D(15,(3,3),activation='relu'))\n","  model.add(MaxPooling2D(pool_size=(2,2)))\n","  model.add(Dropout(0.5))\n","  \n","  model.add(Flatten())\n","  \n","  model.add(Dense(500,activation='relu'))\n","  model.add(Dense(n_class,activation='softmax'))\n","  model.compile(Adam(lr=0.6),loss='categorical_crossentropy',metrics=['accuracy'])\n","  return model\n","  "],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"-dTdTaBuWs57","colab_type":"code","outputId":"09428fd5-444f-4d93-dc32-270afbabe98b","executionInfo":{"status":"ok","timestamp":1566678728447,"user_tz":-330,"elapsed":10600,"user":{"displayName